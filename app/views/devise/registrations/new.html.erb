<% devise_error_messages! %>
<div class='row'>
  <div class='col-lg-3'></div>
  <div class='col-lg-6'>
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title"><%= t('views.users.new.title')%></h3>
      </div>
      <div class="panel-body">
        <div class'row'>
          <div class='col-lg-12'>
            <%= render 'form_new' %>
          </div>
        </div>
        <div class'row'>
          <div class='col-lg-12'>
            <%= link_to t('views.users.new.register'), "#",:onclick=>"$('form').submit()" ,:class => 'btn btn-success btn-lg btn-block' %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class='col-lg-3'></div>
</div>
